//JK flipflop test bench
module testbench();
reg J,K;
reg clk;
wire Q;
wire Q_not;
flipflop uut(J,K,clk,Q,Q_not);
initial begin
 clk=0;
 forever #5 clk = ~clk; 
end
initial 
begin
$monitor("%0dns:J=%b,K=%b,clk=%b,Q=%b,Q_not=%b",
                        $time,J,K,clk,Q,Q_not);
#2 J=0;K=0;
#10 J=0;K=1;
#10 J=1;K=0;
#10 J=1;K=1;
#10 J=0;K=0;
#20 $finish;
end                     
endmodule
