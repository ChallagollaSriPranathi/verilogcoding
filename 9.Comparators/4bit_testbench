//4 bit comparator test bench
module testbench();
reg [3:0]A,B;
wire L,E,G;
comparator uut(A,B,L,E,G);
initial
begin
$monitor("%0dns:A=%b,B=%b,L=%b,E=%b,G=%b",$time,A,B,L,E,G);
//some test cases
#10 A=4'b0000;B=4'b0000;
#10 A=4'b1111;B=4'b1111;

#10 A=4'b1100;B=4'b0011;
#10 A=4'b1010;B=4'b0101;

#10 A=4'b0101;B=4'b0110;
#10 A=4'b1000;B=4'b1001;
$finish;
end
endmodule
