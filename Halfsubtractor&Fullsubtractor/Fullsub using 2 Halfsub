//descriptio of Fullsubtractr from 2 halfsubtractor
module full_sub(
    input A, B,
    output Diff,Borrow
);
    assign Diff=(A^B);
    assign Borrow=(~A)&B;
endmodule
module full_sub(
    input A, B, Bin,
    output Diff,Borrow
);
    wire d1, d2, d3;
    half_sub HS1(A, B, d1, d2);
    half_sub HS2(d1, Bin, Diff, d3);
    assign Borrow=(d2|d3);
endmodule
