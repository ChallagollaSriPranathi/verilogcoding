module testbench();
  reg [9:0] y;
  wire [3:0] A;
  encoder_10x4 uut(y, A);
  initial begin
    $monitor("%0dns: y = %b, A = %b", $time, y, A);
    y = 10'b0000000001; #10;
    y = 10'b0000000010; #10;
    y = 10'b0000000100; #10;
    y = 10'b0000001000; #10;
    y = 10'b0000010000; #10;
    y = 10'b0000100000; #10;
    y = 10'b0001000000; #10;
    y = 10'b0010000000; #10;
    y = 10'b0100000000; #10;
    y = 10'b1000000000; #10;

    $finish;
  end
endmodule
